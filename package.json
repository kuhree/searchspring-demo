{
  "name": "searchspring-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint --ext .ts,tsx --ignore-path .gitignore .",
    "format": "prettier -u --write --ignore-path .gitignore \"**/*.{js,ts,tsx,css,html}\"",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "serve": "vite",
    "serve:preview": "vite preview",
    "serve:prod": "pnpm build && pnpm dlx serve dist"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.8.2",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.2",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-react": "^7.32.2",
    "happy-dom": "^8.9.0",
    "postcss": "^8.4.31",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "vite": "^4.5.6",
    "vitest": "^0.29.1",
    "vitest-dom": "^0.0.4"
  }
}
